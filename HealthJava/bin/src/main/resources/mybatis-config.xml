  <configuration>
  	<typeAliases>
  	<!-- 이 파일의 역할은 데이터 저장빈 클래스 객체 별칭을 관리함으로써 mybatis 매퍼태그 속성에 들어가는 코드를 줄일 수 있다. -->
  	
  	<!-- admin --><!-- admin --><!-- admin --><!-- admin --><!-- admin --><!-- admin --><!-- admin --><!-- admin -->
