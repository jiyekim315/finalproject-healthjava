  	<typeAlias alias="product" type="shop.HealthJava.vo.ProductVO" />
  	
  		<!-- Mypage : 목차별 순서 --> 
		<!-- <typeAlias type="shop.HealthJava.vo.MypageVO" alias="mypage" />-->
